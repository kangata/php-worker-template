{
    "name": "quetzalarc/php-worker",
    "require": {
        "guzzlehttp/guzzle": "^7.1",
        "illuminate/database": "^8.8",
        "illuminate/validation": "^8.8",
        "php-amqplib/php-amqplib": "^2.12",
        "vlucas/phpdotenv": "^5.2"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
