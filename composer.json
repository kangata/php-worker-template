{
    "require": {
        "guzzlehttp/guzzle": "^7.1",
        "illuminate/database": "^7.25",
        "illuminate/validation": "^7.25",
        "php-amqplib/php-amqplib": ">=2.9.0",
        "vlucas/phpdotenv": "^5.1"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
